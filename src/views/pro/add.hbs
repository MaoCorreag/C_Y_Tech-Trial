<div class="container-fluid">
          <!-- Content Row -->
 <div class="row">
     <div class="container p-2">
        <div class="row">
          
            <div class="col-md-6">
                <div class="card-body">
                    <h1>
                        Usuarios Registrados
                    </h1>
                        <table class="table">
                            <thead>
                                <tr>
                                <th scope="col">id</th>
                                <th scope="col">nombres</th>
                                <th scope="col">apellidos</th>
                                <th scope="col">Email</th>
                                <th scope="col">Telefono</th>
                                <th scope="col">Profesi√≥n</th>
                                <th scope="col">Edad</th>
                                <th scope="col">Salario</th>
                                <th scope="col">Banco</th>
                                <th scope="col">F. Contacto Inicial</th>
                                <th scope="col">F. Ultimo Contacto</th>
                                <th scope="col">F. Proximo Contacto</th>
                                <th scope="col">Ciudad</th>
                                <th scope="col">Departamento</th>
                                <th scope="col">#</th>
                                
                                </tr>
                            </thead>
                            <tbody>
                                {{#each empleados}}
                                <tr>
                                <th scope="row">{{id}}</th>
                                <td>{{nombre}}</td>
                                <td>{{apellidos}}</td>
                                <td>{{cedula}}</td>
                                <td>{{celular}}</td>
                                <td>
                                    <a href="delete/{{id}}" class="far fa-trash-alt btn btn-danger"></a>
                                    <a href="/empleados/update" class="btn btn-info">Modificar</a>
                                </td>
                                </tr>
                                {{/each}}
                            </tbody>
                            
                        </table>
                </div>
            </div>
        </div>
    </div>
 </div>
</div>
